<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE timeTable [
  <!ELEMENT timeTable (complexes+, lessons+)>
<!ELEMENT complexes (complex+)>
<!ELEMENT lessons (lesson+)>
<!ELEMENT lesson (date, time)>
<!ELEMENT complex (name, address)>
<!ELEMENT date (#PCDATA)>
<!ELEMENT time (#PCDATA)>
<!ELEMENT name (#PCDATA)>
<!ELEMENT address (#PCDATA)>
<!ATTLIST complex id ID #REQUIRED>
<!ATTLIST lesson
  code CDATA #REQUIRED
  complex IDREF #REQUIRED
  >
]>

<timeTable>
  <complexes>
  <complex id="P001">
    <name>
    Тех отдел
    </name>
    <address>
    Дом 5 улц 6
    </address>
  </complex>
  </complexes>
<lessons>
      <lesson code="001" complex="P001">
        <date>
        31 декабря 025
        </date>
        <time>
        20:00
        </time>
      </lesson>
    </lessons>
    </timeTable>
